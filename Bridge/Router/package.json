{
  "name": "blockflow-studio",
  "version": "1.0.4",
  "description": "BlockFlow Studio Project",
  "scripts": {
    "compile": "tsc -b",
    "instance-test-linea": "blockflow instance-test --path 'linea.studio.yaml' --range 10",
    "instance-deploy-linea": "blockflow instance-deploy --path 'linea.studio.yaml'",
    "instance-test-ethereum": "blockflow instance-test --path 'ethereum.studio.yaml' --range 10 --startBlock 19090010",
    "instance-test-base": "blockflow instance-test --path 'base.studio.yaml' --range 10 --startBlock 15740530",
    "instance-test-ethereum-swapAndDeposit": "blockflow instance-test --path 'ethereum.studio.yaml' --range 1 --startBlock 19568913",
    "instance-test-ethereum-deposit": "blockflow instance-test --path 'ethereum.studio.yaml' --range 5 --startBlock 20119997",
    "instance-test-ethereum-usdc": "blockflow instance-test --path 'ethereum.studio.yaml' --range 3 --startBlock 20096506",
    "instance-test-ethereum-usdcPaid": "blockflow instance-test --path 'ethereum.studio.yaml' --range 3 --startBlock 19253582",
    "instance-test-ethereum-dexspan": "blockflow instance-test --path 'ethereum.studio.yaml' --range 3 --startBlock 19481971",
    "instance-test-ethereum-transfer": "blockflow instance-test --path 'ethereum.studio.yaml' --range 5 --startBlock 20096150",
    "instance-test-ethereum-transferWithInstruction": "blockflow instance-test --path 'ethereum.studio.yaml' --range 2 --startBlock 20000486",
    "instance-test-ethereum-execute": "blockflow instance-test --path 'ethereum.studio.yaml' --range 5 --startBlock 20069617",
    "instance-test-base-executeWithMessage": "blockflow instance-test --path 'base.studio.yaml' --range 5 --startBlock 15143109",
    "instance-test-linking": "blockflow instance-test --path 'ethereum.studio.yaml' --range 3 --startBlock 20121926 && blockflow instance-test --path 'base.studio.yaml' --range 3 --startBlock 15982046",
    "instance-deploy-ethereum": "blockflow instance-deploy --path 'ethereum.studio.yaml'",
    "instance-deploy-optimism": "blockflow instance-deploy --path 'op.studio.yaml'",
    "instance-test-optimism": "blockflow instance-test --path 'op.studio.yaml' --range 10",
    "instance-test-optimism-swapWithReceipt": "blockflow instance-test --path 'op.studio.yaml' --range 1 --startBlock 117775092",
    "api-test": "blockflow api-test --path 'api.studio.yaml'",
    "api-deploy": "blockflow api-deploy --path 'api.studio.yaml'"
  },
  "dependencies": {
    "@blockflow-labs/cli": "^1.0.8-beta.5",
    "@blockflow-labs/utils": "^1.0.7",
    "@typegoose/typegoose": "^11.7.0",
    "axios": "^1.7.2",
    "ethers": "^6.11.1",
    "request": "^2.88.2",
    "web3": "^4.9.0"
  },
  "devDependencies": {
    "bignumber.js": "^9.1.1",
    "prettier": "^3.0.3",
    "ts-loader": "^9.5.0",
    "typescript": "^5.1.6",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
